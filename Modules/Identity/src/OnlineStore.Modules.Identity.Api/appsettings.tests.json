{
  "AppOptions": {
    "Name": "Online Store",
    "Description": "Online Store Modular Monolith application with Domain-Driven Design and CQRS based on .Net",
    "ApiAddress": "http://localhost:7000"
  },
  "ConnectionStrings": {
    "RedisConnectionString": "127.0.0.1:6379,ssl=False"
  },
  "mssql": {
    "ConnectionString": "Data Source=.\\sqlexpress;Initial Catalog=OnlineStore-Test;Integrated Security=True;Connect Timeout=30"
  },
  "logger": {
    "level": "information",
    "excludePaths": ["/", "/ping", "/metrics"],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "apiKey": "secret"
    },
    "tags": {}
  },
  "AllowedHosts": "*",
  "CORS": {
    "AllowAnyOrigin": false,
    "AllowedOrigins": [ "http://localhost:4200", "http://localhost:3000" ]
  },
  "IdentityOptions": {
    "Password": {
      "RequiredLength": 8,
      "RequireDigit": false,
      "RequireNonAlphanumeric": false
    },
    "User": {
      "MaxPasswordAge": "0",
      "RequireUniqueEmail": true
    },
    "Lockout": {
      "DefaultLockoutTimeSpan": "0:15:0"
    }
  },
  "Authorization": {
    "RefreshTokenLifeTime": "30.00:00:00",
    "AccessTokenLifeTime": "00:30:00",
    "LimitedCookiePermissions": "asset:read;content:read;asset:create;"
  },
  "messaging": {
    "useBackgroundDispatcher": true,
    "inbox": {
      "enabled": false,
      "collectionName": "inbox"
    },
    "outbox": {
      "enabled": true,
      "collectionName": "outbox",
      "interval": "00:00:01"
    }
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "online-store-test",
    "disableTransactions": true
  },
  "JWTSettings": {
    "Key": "1105D15CB0D48F5781C103A18D5599E4FF25C9102FA694ABDF1DA6828BF153DE",
    "Issuer": "Identity",
    "Audience": "IdentityUser",
    "DurationInMinutes": 60
  },
  "MailConfiguration": {
    "From": "test@test.com",
    "Host": "smtp.test.com",
    "Port": 587,
    "UserName": "",
    "Password": "",
    "DisplayName": ""
  },
  "easycaching": {
    "inmemory": {
      "MaxRdSecond": 120,
      "EnableLogging": false,
      "LockMs": 5000,
      "SleepMs": 300,
      "DBConfig": {
        "SizeLimit": 10000,
        "ExpirationScanFrequency": 60,
        "EnableReadDeepClone": true,
        "EnableWriteDeepClone": false
      }
    },
    "redis": {
      "MaxRdSecond": 120,
      "EnableLogging": false,
      "LockMs": 5000,
      "SleepMs": 300,
      "dbconfig": {
        "Password": null,
        "IsSsl": false,
        "SslHost": null,
        "ConnectionTimeout": 5000,
        "AllowAdmin": true,
        "Endpoints": [
          {
            "Host": "localhost",
            "Port": 6739
          }
        ],
        "Database": 0
      }
    }
  }
}
